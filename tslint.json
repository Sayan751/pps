{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "curly": false,
        "max-line-length": [true, 200],
        "eofline": false,
        "max-classes-per-file": [false],
        "trailing-comma": [false],
        "no-console": [false],
        "interface-name": ["never-prefix"],
        "one-variable-per-declaration": [false]
    },
    "rulesDirectory": []
}